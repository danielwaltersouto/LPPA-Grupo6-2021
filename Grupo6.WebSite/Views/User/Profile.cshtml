@model Grupo6.Entities.Models.Usuario

<head>
    <!--<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">-->
    <link href="~/Content/LogIn/EstiloLogin.css" rel="stylesheet" />
</head>


<body>

    <div class="container">

        <div class="row">

            <div class="col-md-8">

                <a class="btn btn-primary" href="~/User/Profile">
                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-file-person" viewBox="0 0 20 20">
                        <path d="M12 1a1 1 0 0 1 1 1v10.755S12 11 8 11s-5 1.755-5 1.755V2a1 1 0 0 1 1-1h8zM4 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H4z" />
                        <path d="M8 10a3 3 0 1 0 0-6 3 3 0 0 0 0 6z" />
                    </svg>Modificar Datos de Usuario
                </a>
            

       


      
            
                <a class="btn btn-primary" href="@Url.Action("ChangePass", "Auth")">
                    <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-file-lock2" viewBox="0 0 16 16">
                        <path d="M8 5a1 1 0 0 1 1 1v1H7V6a1 1 0 0 1 1-1zm2 2.076V6a2 2 0 1 0-4 0v1.076c-.54.166-1 .597-1 1.224v2.4c0 .816.781 1.3 1.5 1.3h3c.719 0 1.5-.484 1.5-1.3V8.3c0-.627-.46-1.058-1-1.224z" />
                        <path d="M4 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H4zm0 1h8a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1z" />
                    </svg>Cambiar Contraseña
                </a>
            </div>

          </div>

            <div class="row">

                <div class="col-md-9  admin-content" id="profile">
                    <div class="panel panel-info" style="margin: 1em;">
                        <div class="panel-heading">
                            <h3 class="panel-title">Email</h3>
                        </div>
                        <div class="panel-body">
                            @ViewBag.Usuario.Email

                        </div>
                    </div>
                </div>


                <div class="col-md-9  admin-content" id="profile">
                    <div class="panel panel-info" style="margin: 1em;">
                        <div class="panel-heading">
                            <h3 class="panel-title">Nombre</h3>
                        </div>
                        <div class="panel-body">
                            @ViewBag.Usuario.Nombre

                        </div>
                    </div>
                </div>


                <div class="col-md-9  admin-content" id="profile">
                    <div class="panel panel-info" style="margin: 1em;">
                        <div class="panel-heading">
                            <h3 class="panel-title">Apellido</h3>
                        </div>
                        <div class="panel-body">
                            @ViewBag.Usuario.Apellido


                        </div>
                    </div>
                </div>


                <div class="col-md-9  admin-content" id="profile">
                    <div class="panel panel-info" style="margin: 1em;">
                        <div class="panel-heading">
                            <h3 class="panel-title">Documento</h3>
                        </div>
                        <div class="panel-body">
                            @ViewBag.Usuario.Documento


                        </div>
                    </div>
                </div>


                <div class="col-md-9  admin-content" id="profile">
                    <div class="panel panel-info" style="margin: 1em;">
                        <div class="panel-heading">
                            <h3 class="panel-title">Direccion Principal</h3>
                        </div>
                        <div class="panel-body">

                            @ViewBag.Usuario.Direccion
                        </div>
                    </div>
                </div>


                <div class="col-md-9  admin-content" id="profile">
                    <div class="panel panel-info" style="margin: 1em;">
                        <div class="panel-heading">
                            <h3 class="panel-title">Direccion Secundaria</h3>
                        </div>
                        <div class="panel-body">


                        </div>
                    </div>
                </div>




                <div class="col-md-9  admin-content" id="profile">
                    <div class="panel panel-info" style="margin: 1em;">
                        <div class="panel-heading">
                            <h3 class="panel-title">Tipo de Facturacion</h3>
                        </div>
                        <div class="panel-body">


                            var items = (from @ViewBag.Usuario.CategoriaFiscal value in values
                            select new SelectListItem)


                            @ViewBag.Usuario.CategoriaFiscal

                        </div>
                    </div>
                </div>
            </div>

        </div>

</body>






